{
  "name": "airobot-server",
  "version": "1.0.0",
  "description": "一款轻松部署ai对话机器人的web平台，支持多模型chatgpt、claude、stablediffusion等 支持多平台机器人微信、qq等",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "aggregate-base": "^1.2.0",
    "egg": "^3",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^4.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2",
    "egg-session-redis": "^2.1.0",
    "egg-super-router": "^1.1.2",
    "joi": "^17.8.3",
    "moment": "^2.29.4",
    "openai": "^3.2.1"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-airobot-server --port=7002",
    "start-docker": "egg-scripts start --title=egg-server-airobot-server",
    "stop": "egg-scripts stop --title=egg-server-airobot-server",
    "dev": "egg-bin dev --port=7002",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "xiang",
  "license": "MIT"
}
